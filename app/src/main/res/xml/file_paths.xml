<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 캐시 폴더 내의 shared_photos 폴더 -->
    <cache-path
        name="shared_photos"
        path="shared_photos/" />
    <!-- 전체 캐시 폴더 접근 -->
    <cache-path
        name="cache"
        path="." />
    <!-- 외부 저장소 DCIM 폴더 (사진 공유용) -->
    <external-path
        name="external_dcim"
        path="DCIM/" />
    <!-- 전체 외부 저장소 접근 (사진 공유용) -->
    <external-path
        name="external_storage"
        path="." />
</paths>